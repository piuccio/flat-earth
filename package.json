{
  "name": "flat-earth",
  "version": "0.0.0",
  "description": "Modern set of functions for handling coordinates in Node.js or the browser",
  "scripts": {
    "build": "rollup -c",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "main": "build/bundle.cjs.js",
  "module": "build/bundle.es.js",
  "browser": "build/bundle.umd.js",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.42",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0-beta.42",
    "@babel/preset-env": "^7.0.0-beta.42",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^22.4.3",
    "jest": "^22.4.3",
    "regenerator-runtime": "^0.11.1",
    "rollup": "^0.57.1",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.3.0"
  },
  "dependencies": {
    "latlon-geohash": "^1.1.0"
  },
  "engines": {
    "node": ">6"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "es2015": {
            "modules": false
          }
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": [
          "@babel/plugin-transform-modules-commonjs"
        ]
      }
    }
  },
  "jest": {
    "testRegex": "tests/.*\\.test\\.js$"
  }
}
